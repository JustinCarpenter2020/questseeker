{
    "name": "new",
    "version": "1.0.0",
    "scripts": {
      "build": "npm run setup:client && npm run setup:server && npm run build:client",
      "setup:client": "cd new.client && npm i",
      "setup:server": "cd new.server && npm i",
      "build:client": "npm run --prefix new.client build",
      "start": "node new.server/index.js"
    },
    "engines": {
      "node": "^14.15.4"
    }
  }